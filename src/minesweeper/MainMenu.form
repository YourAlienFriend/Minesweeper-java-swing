            <Component class="javax.swing.JLabel" name="jLabel8">
              <Properties>
                <Property name="text" type="java.lang.String" value="&lt;html&gt;&#xa;&lt;h style=&quot;color:red;&quot;&gt;&#x39f;&#x3b4;&#x3b7;&#x3b3;&#x3af;&#x3b5;&#x3c2; &#x3c0;&#x3c1;&#x3bf;&#x3c2; &#x3bd;&#x3ad;&#x3bf;&#x3c5;&#x3c2; &#x3bd;&#x3b1;&#x3c1;&#x3ba;&#x3b1;&#x3bb;&#x3b9;&#x3b5;&#x3c5;&#x3c4;&#x3ad;&#x3c2;:&lt;/h&gt;&#xa;&lt;p&gt;&#x9;&amp;emsp;&#x9;&#x3a3;&#x3ba;&#x3bf;&#x3c0;&#x3cc;&#x3c2; &#x3c4;&#x3bf;&#x3c5;  &#x3c0;&#x3b1;&#x3b9;&#x3c7;&#x3bd;&#x3b9;&#x3b4;&#x3bf;&#x3cd;: &lt;/p&gt;&#xa; &lt;br&gt;&lt;/br&gt;&#xa;&lt;p&gt;&#x9;&#x3a3;&#x3ba;&#x3bf;&#x3c0;&#x3cc;&#x3c2; &#x3c4;&#x3bf;&#x3c5; &#x3c0;&#x3b1;&#x3b9;&#x3c7;&#x3bd;&#x3b9;&#x3b4;&#x3b9;&#x3bf;&#x3cd; &#x3b5;&#x3af;&#x3bd;&#x3b1;&#x3b9; &#x3b7; &#x3b1;&#x3c0;&#x3bf;&#x3ba;&#x3ac;&#x3bb;&#x3c5;&#x3c8;&#x3b7; &#x3cc;&#x3bb;&#x3c9;&#x3bd; &#x3c4;&#x3c9;&#x3bd; &#x3b4;&#x3b9;&#x3b1;&#x3c7;&#x3c9;&#x3c1;&#x3b9;&#x3c3;&#x3bc;&#x3ce;&#x3bd; &#x3c4;&#x3b7;&#x3c2; &#x3b8;&#x3ac;&#x3bb;&#x3b1;&#x3c3;&#x3c3;&#x3b1;&#x3c2;(&#x3ba;&#x3b5;&#x3bb;&#x3b9;&#x3ce;&#x3bd;) &#x3b5;&#x3ba;&#x3c4;&#x3cc;&#x3c2; &#x3b1;&#x3c5;&#x3c4;&#x3ce;&#x3bd; &#x3c0;&#x3bf;&#x3c5; &#x3c0;&#x3b5;&#x3c1;&#x3b9;&#x3ad;&#x3c7;&#x3bf;&#x3c5;&#x3bd; &#x3bd;&#x3ac;&#x3c1;&#x3ba;&#x3b7;.&#x39a;&#x3ac;&#x3b8;&#x3b5; &#x3c6;&#x3bf;&#x3c1;&#x3ac; &#x3c0;&#x3bf;&#x3c5; &#x3b1;&#x3c0;&#x3bf;&#x3ba;&#x3b1;&#x3bb;&#x3cd;&#x3c0;&#x3c4;&#x3b5;&#x3c4;&#x3b5; &#x3ad;&#x3bd;&#x3b1; &#x3ba;&#x3b5;&#x3bb;&#x3af; &#x3ba;&#x3b1;&#x3b9; &#x3b1;&#x3c5;&#x3c4;&#x3cc; &#x3b1;&#x3bd; &#x3b4;&#x3ad;&#x3bd; &#x3b5;&#x3af;&#x3bd;&#x3b1;&#x3b9; &#x3bd;&#x3ac;&#x3c1;&#x3ba;&#x3b7; &#x3c4;&#x3cc;&#x3c4;&#x3b5; &#x3c3;&#x3b1;&#x3c2; &#x3b5;&#x3bc;&#x3c6;&#x3b1;&#x3bd;&#x3af;&#x3b6;&#x3b5;&#x3c4;&#x3b1;&#x3b9; &#x3bf; &#x3b1;&#x3c1;&#x3b9;&#x3b8;&#x3bc;&#x3cc;&#x3c2; &#x3bd;&#x3b1;&#x3c1;&#x3ba;&#x3ce;&#x3bd; &#x3c3;&#x3b5; &#x3b3;&#x3b5;&#x3b9;&#x3c4;&#x3bf;&#x3bd;&#x3b9;&#x3ba;&#x3ac; &#x3ba;&#x3b5;&#x3bb;&#x3b9;&#x3ac; (&#x3c0;&#x3ac;&#x3bd;&#x3c9;,&#x3ba;&#x3ac;&#x3c4;&#x3c9;,&#x3b4;&#x3b5;&#x3be;&#x3b9;&#x3ac;,&#x3b1;&#x3c1;&#x3b9;&#x3c3;&#x3c4;&#x3b5;&#x3c1;&#x3ac; &#x3ba;&#x3b1;&#x3b9; &#x3b4;&#x3b9;&#x3b1;&#x3b3;&#x3ce;&#x3bd;&#x3b9;&#x3b1;) &#x3ae; &#x3c3;&#x3b1;&#x3c2; &#x3b5;&#x3bc;&#x3c6;&#x3b1;&#x3bd;&#x3af;&#x3b6;&#x3bf;&#x3bd;&#x3c4;&#x3b1;&#x3b9; &#x3cc;&#x3bb;&#x3b1; &#x3c4;&#x3b1; &#x3ba;&#x3b5;&#x3bd;&#x3ac; &#x3ad;&#x3c9;&#x3c2; &#x3b5;&#x3ba;&#x3ad;&#x3b9; &#x3c0;&#x3bf;&#x3c5; &#x3b3;&#x3b5;&#x3b9;&#x3c4;&#x3bf;&#x3bd;&#x3b5;&#x3cd;&#x3b5;&#x3b9; &#x3bd;&#x3ac;&#x3c1;&#x3ba;&#x3b7;.&#x391;&#x3bd;&#x3c4;&#x3af;&#x3b8;&#x3b5;&#x3c4;&#x3b1; &#x3b1;&#x3bd; &#x3b1;&#x3c0;&#x3bf;&#x3ba;&#x3b1;&#x3bb;&#x3cd;&#x3c8;&#x3b5;&#x3c4;&#x3b5; &#x3bc;&#x3b9;&#x3ac; &#x3bd;&#x3ac;&#x3c1;&#x3ba;&#x3b7;, &#x3b1;&#x3c5;&#x3c4;&#x3ae; &#x3b5;&#x3ba;&#x3c1;&#x3ae;&#x3b3;&#x3bd;&#x3c5;&#x3c4;&#x3b1;&#x3b9; &#x3ba;&#x3b1;&#x3b9; &#x3c4;&#x3bf; &#x3c0;&#x3b1;&#x3b9;&#x3c7;&#x3bd;&#x3af;&#x3b4;&#x3b9; &#x3c4;&#x3b5;&#x3bb;&#x3b5;&#x3b9;&#x3ce;&#x3bd;&#x3b5;&#x3b9; .&#x393;&#x3b9; &#x3b1;&#x3c5;&#x3c4;&#x3cc; &#x3a0;&#x3a1;&#x39f;&#x3a3;&#x39f;&#x3a7;&#x397;!!! &#x391;&#x3bd; &#x3b5;&#x3af;&#x3c3;&#x3c4;&#x3b5; &#x3c3;&#x3af;&#x3b3;&#x3bf;&#x3c5;&#x3c1;&#x3bf;&#x3b9; &#x3b3;&#x3b9;&#x3b1; &#x3c4;&#x3bf; &#x3c0;&#x3bf;&#x3c5; &#x3c5;&#x3c0;&#x3ac;&#x3c1;&#x3c7;&#x3b5;&#x3b9; &#x3bd;&#x3ac;&#x3c1;&#x3ba;&#x3b7; &#x3c4;&#x3bf;&#x3c0;&#x3bf;&#x3b8;&#x3b5;&#x3c4;&#x3ae;&#x3c3;&#x3c4;&#x3b5; &#x3c3;&#x3b7;&#x3bc;&#x3b1;&#x3b9;&#x3ac;&#x3ba;&#x3b9; &#x3ce;&#x3c3;&#x3c4;&#x3b5; &#x3bd;&#x3b1; &#x3bc;&#x3b7; &#x3bc;&#x3c0;&#x3bf;&#x3c1;&#x3b5;&#x3af;&#x3c4;&#x3b1;&#x3b9; &#x3bd;&#x3b1; &#x3c4;&#x3b7; &#x3c0;&#x3b1;&#x3c4;&#x3ae;&#x3c3;&#x3b5;&#x3c4;&#x3b5;.&lt;p&gt;&#xa;&lt;br&gt;&lt;/br&gt;&#xa;&lt;p&gt;&amp;emsp;&#x9;&#x3a7;&#x3b5;&#x3b9;&#x3c1;&#x3b9;&#x3c3;&#x3bc;&#x3cc;&#x3c2;:&lt;/p&gt;&#xa;&lt;br&gt;&lt;/br&gt;&#xa;&lt;p&gt;                  &amp;emsp; &amp;emsp;&amp;emsp;&#x391;&#x3c1;&#x3b9;&#x3c3;&#x3c4;&#x3b5;&#x3c1;&#x3cc; &#x3ba;&#x3bb;&#x3b9;&#x3ba;: &#x391;&#x3c0;&#x3bf;&#x3ba;&#x3ac;&#x3bb;&#x3c5;&#x3c8;&#x3b7; &#x3ba;&#x3b5;&#x3bb;&#x3b9;&#x3bf;&#x3cd;&#xa;&lt;br&gt;&lt;/br&gt;&#xa;                          &amp;emsp; &amp;emsp;&amp;emsp; &#x394;&#x3b5;&#x3be;&#x3af; &#x3ba;&#x3bb;&#x3b9;&#x3ba;: &#x3a4;&#x3bf;&#x3c0;&#x3bf;&#x3b8;&#x3ad;&#x3c4;&#x3b7;&#x3c3;&#x3b7; &#x3a3;&#x3b7;&#x3bc;&#x3b1;&#x3af;&#x3b1;&#x3c2;&#xa;&lt;br&gt;&lt;/br&gt;&#xa;&#x9;&amp;emsp; &#x3a1;&#x3c5;&#x3b8;&#x3bc;&#x3af;&#x3c3;&#x3b5;&#x3b9;&#x3c2;:&#xa;&lt;br&gt;&lt;/br&gt;&#xa;&#x9;&#x391;&#x3c0;&#x3cc; &#x3c4;&#x3bf; &#x3bc;&#x3b5;&#x3bd;&#x3bf;&#x3cd; &#x3b5;&#x3c0;&#x3b9;&#x3bb;&#x3bf;&#x3b3;&#x3ad;&#x3c2; &#x3bc;&#x3c0;&#x3bf;&#x3c1;&#x3b5;&#x3af;&#x3c4;&#x3b5; &#x3bd;&#x3b1; &#x3b5;&#x3c0;&#x3b9;&#x3bb;&#x3ad;&#x3be;&#x3b5;&#x3c4;&#x3b5; &#x3b5;&#x3c0;&#x3af;&#x3c0;&#x3b5;&#x3b4;&#x3bf; &#x3b4;&#x3c5;&#x3c3;&#x3ba;&#x3bf;&#x3bb;&#x3af;&#x3b1;&#x3c2; &#x3c0;&#x3bf;&#x3c5; &#x3c3;&#x3c5;&#x3bd;&#x3b5;&#x3c0;&#x3ac;&#x3b3;&#x3b5;&#x3c4;&#x3b1;&#x3b9; &#x3bc;&#x3b5; &#x3c4;&#x3bf;&#x3bd;  &#x3c3;&#x3c5;&#x3bd;&#x3bf;&#x3bb;&#x3b9;&#x3ba;&#x3cc; &#x3b1;&#x3c1;&#x3b9;&#x3b8;&#x3bc;&#x3cc; &#x3bd;&#x3b1;&#x3c1;&#x3ba;&#x3ce;&#x3bd; (10,40 &#x3ae; 90)&#xa;&#x3ba;&#x3b1;&#x3b8;&#x3ce;&#x3c2; &#x3ba;&#x3b1;&#x3b9; &#x3bd;&#x3b1; &#x3b5;&#x3be;&#x3b1;&#x3c4;&#x3bf;&#x3bc;&#x3b9;&#x3ba;&#x3b5;&#x3cd;&#x3c3;&#x3b5;&#x3c4;&#x3b5; &#x3c4;&#x3bf; &#x3c7;&#x3c1;&#x3ce;&#x3bc;&#x3b1; &#x3b1;&#x3c5;&#x3c4;&#x3ce;&#x3bd;.&#x39a;&#x3b1;&#x3bb;&#x3cc; &#x3c8;&#x3ac;&#x3c1;&#x3b5;&#x3bc;&#x3b1;!! &lt;/p&gt;&#xa;&lt;/html&gt;"/>
